{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-self-close": false,
    "member-access": false,
    "ordered-imports": false,
    "no-console": false,
    "semicolon": [
      true,
      "never"
    ]
  }
}